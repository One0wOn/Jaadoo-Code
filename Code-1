#include<stdio.h>
int main()
{
    int a,s=1,c1=0;
    scanf("%d",&a);
    if(a==0)
    {
        while(!(c1==1))
        {
            scanf("%d",&a);
            if(a==0)
            {
                c1=1;
            }
            else
            s=s*a;
        }
        printf("%d",s);
    }
    if(!a==0)
    {
        while(!(c1==1))
        {
            scanf("%d",&a);
            if(a==0)
                c1=1;
        }
        while(!(c1==2))
        {
            scanf("%d",&a);
            if(a==0)
                c1=2;
            else
                s=s*a;
        }
        printf("%d",s);
    }
    
    return 0;
}
